package q17;

public class Interest {

	
	public double InterestCalc(double principal,double roi,double years) {
		double g=principal*.1;
		double e=roi*.1;
		return g*e*years;
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		InterestCalc(25000,5,10);

	}

}
