package q2;
import java.util.*;

public class Fibon {

	ArrayList<Integer> lis= new ArrayList<>();
	
	static int[] ge=new int[25];
	public int a;
	
	public static void Fibon(){
		
		int a=4;
		ge[0]=1;
		ge[1]=1;
		
		while (a<25) {
		ge[a]=ge[a-1]+ge[a-2];
		
		System.out.println(ge[a]);
		a++;
			
		}
		
	}
	
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Fibon();

	}

}
