	static void Examps(String name, int age) {
		int g=0;
		
		while(g<name.length()) {
			if(g==0) {
				System.out.println("The 1 letter in your name is "+name.substring(0,1));
			g++;
			}
			
			else {
				int holder=g+1;
			System.out.print("The "+holder+" letter in your name is ");
			System.out.println(name.substring(g,g+1));
			g++;
			}
		}
		//enum 

		int holder=name.length();

		
		switch(holder) {
		case 1-6:
			System.out.println("You have a pretty short name");
		break;
		case 6-10:
			System.out.println("Your name is not to short or too long just right");
		break;
		case 10-100:
			System.out.println("Your name is way to long boss");
			
		
		default:System.out.println("blah");
		}

		
		
	}
	
	
