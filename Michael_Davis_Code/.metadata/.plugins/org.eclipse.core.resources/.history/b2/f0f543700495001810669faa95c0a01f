		super.checkAccNumber();
	}



	@Override
	public void resetPin() {
		// TODO Auto-generated method stub
		super.resetPin();
	}



	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Checkings a=new Checkings();
		a.

	}

}
