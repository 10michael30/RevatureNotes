package q19;

import java.util.ArrayList;

import q9.Primes;

public class ArrayWork {
	ArrayList<Integer> a=new ArrayList<>();
	ArrayList<Integer> b;
	
	public ArrayWork() {
		
		a.add(1);
		a.add(2);
		a.add(3);
		a.add(4);
		a.add(5);
		a.add(6);
		a.add(7);
		a.add(8);
		a.add(9);
		a.add(10);
		b=a;
		
		int eventotal=0;
		int oddtotal=0;
		Primes a=new Primes();
		
		for(int b:a) {
			if(b%2==0) {
				eventotal+=b;
			}
			else oddtotal+=b;
			
		}
		
		for(int g:b) {
			if(a.isPrime(g)) {
				b.remove(g);
			}
		}
		
	}
}
