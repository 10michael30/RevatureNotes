import java.util.*;
public class demonstrate {
	public demonstrate(String name, int age) {
		int g=0;
		while(g<name.length()) {
			System.out.print("The "+g+" letter in your name is ");
			System.out.print(name.charAt(g));
			g++;
		}
		
		
		
		
		
	}

	
	
