package com.ex.beans;

public class Genre {
private int id;
private String name;
public Genre() {
	super();
}
public Genre(String name) {
	super();
	this.name = name;
}
public int getId() {
	return id;
}
public void setId(int id) {
	this.id = id;
}
public String getName() {
	return name;
}
public void setName(String name) {
	this.name = name;
}
}
